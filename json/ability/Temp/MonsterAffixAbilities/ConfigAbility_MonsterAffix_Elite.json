{
	"Default":{
		"$type": "ConfigAbility",
		"abilityName": "MonsterAffix_Monster_DieTogether_Fake",
		"isDynamicAbility": true,
		"onAdded": [
			{
				"$type": "ApplyModifier",
				"modifierName": "MonsterAffix_Monster_DieTogether_Fake"
			}
		],
		"abilityMixins": [
			{
				"$type": "AttachToAIAlertnessMixin",
				"alertness": [2],
				"modifierName": "MonsterAffix_Monster_DieTogether_FakeLinkEff_Delay_Handler"
			}
		],
		"modifiers": {
			"MonsterAffix_Monster_DieTogether_FakeLinkEff_Delay_Handler":{
				"onAdded": [
					{
						"$type": "ApplyModifier",
						"modifierName": "MonsterAffix_Monster_DieTogether_FakeLinkEff_Delay"
					}
				]
			},
			"MonsterAffix_Monster_DieTogether_FakeLinkEff_Delay":{
				"duration": 5,
				"thinkInterval": 2.5,
				"onThinkInterval": [
					{
						"$type": "ApplyModifier",
						"otherTargets":{
							"$type": "SelectTargetsByShape",
							"campBasedOn": "Self",
							"campTargetType": "Alliance",
							"centerBasedOn": "Self",
							"shapeName": "CircleR80"
						},
						"modifierName": "MonsterAffix_Monster_DieTogether_FakeLinkEff",
						"predicates": [
							{
								"$type": "ByTargetGlobalValue",
								"key": "_FAKE_DIETOGETHER_TARGET",
								"value": 1,
								"compareType": "Equal"
							}
						]
					}
				]
			},
			"MonsterAffix_Monster_DieTogether_FakeLinkEff":{
				"duration": 1,
				"onAdded": [
					{
						"$type": "FireEffect",
						"target": "Applier",
						"effectPattern": "Eff_Monster_GereralSkill_ReviveHintLInk",
						"born":{
							"$type": "ConfigBornByAttachPoint",
							"attachPointTargetType": "Self",
							"attachPointName": "RootNode"
						}
					}
				]
			},
			"MonsterAffix_Monster_DieTogether_Fake":{
				"onAdded": [
					{
						"$type": "AttachEffect",
						"effectPattern": "Eff_Monster_GeneralSkill_ReviveMark"
					},
					{
						"$type": "SetGlobalValue",
						"key": "_FAKE_DIETOGETHER_TARGET",
						"value": 1
					}
				],
				"onKill": [
					{
						"$type": "ApplyModifier",
						"otherTargets":{
							"$type": "SelectTargetsByShape",
							"campBasedOn": "Self",
							"campTargetType": "Alliance",
							"centerBasedOn": "Self",
							"shapeName": "CircleR80"
						},
						"modifierName": "MonsterAffix_Monster_DieTogether_Survive",
						"predicates": [
							{
								"$type": "ByTargetGlobalValue",
								"key": "_FAKE_DIETOGETHER_TARGET",
								"value": 1,
								"compareType": "Equal"
							}
						]
					},
					{
						"$type": "RemoveModifier",
						"modifierName": "MonsterAffix_Monster_DieTogether_Fake"
					}
				]
			},
			"MonsterAffix_Monster_DieTogether_Survive":{
				"duration": 20,
				"thinkInterval": 3,
				"onThinkInterval": [
					{
						"$type": "ApplyModifier",
						"otherTargets":{
							"$type": "SelectTargetsByShape",
							"campBasedOn": "Self",
							"campTargetType": "AllExceptSelf",
							"centerBasedOn": "Self",
							"shapeName": "CircleR80"
						},
						"modifierName": "MonsterAffix_Monster_DieTogether_FakeLinkEff",
						"predicates": [
							{
								"$type": "ByTargetGlobalValue",
								"key": "_FAKE_REVIVE_GADGET",
								"value": 1,
								"compareType": "Equal"
							}
						]
					}
				]
			}
		}
	}
},
